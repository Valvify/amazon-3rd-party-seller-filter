{
  "name": "Amazon market place filter",
  "version": "1.0",
  "description": "Filtering out 3rd party seller's product on amazon search results. Amazonの検索結果ページから3rd party販売元の商品を除外します",
  "manifest_version": 2,
  "background": {
    "page": "src/html/background.html"
  },
  "permissions": [
    "tabs",
    "*://*.amazon.com/*",
    "*://*.amazon.co.jp/*"
  ],
  "browser_action": {
    "default_icon": "icon/favicon-32x32_on.png",
    "default_title": "Amazon 3rd party seller filter"
  },
  "icons": {
    "16": "icon/favicon-16x16.png",
    "32": "icon/favicon-32x32.png",
    "96": "icon/favicon-96x96.png"
  }
}
